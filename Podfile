source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'

workspace 'PeerlabPD'


use_frameworks!

target 'PeerlabPD' do

  project 'PeerlabPD.xcodeproj'
  pod 'Alamofire',  '~> 4.7'
  pod 'RxSwift'
  pod 'SDWebImage', '~> 4.4'

end


target 'PeerlabPDObjC' do

    project 'PeerlabPDObjC.xcodeproj'
    pod 'Alamofire',  '~> 4.7'
    pod 'RxSwift'
    pod 'SDWebImage', '~> 4.4'

end
